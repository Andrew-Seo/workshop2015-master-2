import org.jointheleague.graphical.robot.Robot;

public class robotMethods
{
	public static void main(String[] args)
	{
		Robot Anderz = new Robot();
		Robot Wonny = new Robot(200, 200);
		Anderz.penDown();
		Anderz.setSpeed(10);
		for (int i = 0; i < 4; i++)
		{

			Anderz.move(500);
			Anderz.turn(90);
		}
	}

}