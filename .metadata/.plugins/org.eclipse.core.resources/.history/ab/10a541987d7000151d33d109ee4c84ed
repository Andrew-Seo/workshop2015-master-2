package Photo_Project;

import javax.swing.JFrame;

public class Photobook
{
	JFrame spen;
	Image_Panel impan;

	public static void main(String[] args)
	{
		Photobook j = new Photobook();
	}

	public Photobook()
	{
		spen = new JFrame();
		spen.setVisible(true);
		imp = new Image_Panel(".jpg");
		spen.add(impan);
	}

}
