	{

		try
		{
			FileReader fr = new FileReader("jjjh.txt");
			while (true)
			{

				System.out.print((char) fr.read());
			}

		} catch (Exception Anderz)
		{
			// TODO Auto-generated catch block
			Anderz.printStackTrace();
		}

	}
}
