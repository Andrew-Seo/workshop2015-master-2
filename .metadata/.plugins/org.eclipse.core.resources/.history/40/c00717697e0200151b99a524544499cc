import java.util.Random;

import javax.swing.JOptionPane;


public class ARandomThing
{
public static void main(String[] args)
{
	
	for (int i = 0; i < 5; i++)
	{
		
	String Question = JOptionPane.showInputDialog("Guess a number between negative 1 million and 1 million");
	int userInput = Integer.parseInt(Question);
	int computerInput = new Random().nextInt(0 - 100);
	if (computerInput == userInput)
	{
		String correct = JOptionPane.showInputDialog("Sat is correct");
		int Correct = Integer.parseInt(correct);
	}
	if (computerInput < userInput);
	{
		String lower = JOptionPane.showInputDialog("lower");
	int Lower = Integer.parseInt(lower);
	}
	if (computerInput > userInput)
	{
		String higher = JOptionPane.showInputDialog("higher");
		
		int Higher = Integer.parseInt(higher);
	}
	
	
	
	
	
	
	
	
	
	
	
}
}
}
