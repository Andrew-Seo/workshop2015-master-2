import javax.swing.JFrame;
import javax.swing.JLabel;

public class JStuff
{
	JFrame k;

	public static void main(String[] args)
	{
		new JStuff();

	}

	JStuff()
	{
		k = new JFrame();
		JLabel n = new JLabel();
		k.setSize(500, 500);
		k.setVisible(true);
		k.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);

	}
}
